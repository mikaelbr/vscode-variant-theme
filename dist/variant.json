{
  "$schema": "vscode://schemas/color-theme",
  "author": "Siamak Mokhtari, Extended by Dan Hedgecock",
  "name": "Panda Syntax â€“ Beta",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.sublime_panda_syntax",
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#868688",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "keyword.control, keyword.operator.new",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "scope": "keyword.operator.logical, keyword.operator.comparison",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": "meta.object-binding-pattern-variable.js variable",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "variable.other.constant, variable.language.this, variable.interpolation",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "border-bottom": "1px dashed rgba(255, 44, 109, 0.5)"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "background-foreground": "rgba(255, 44, 109, 0.62)"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "support.type.object.module.js",
      "settings": {
        "foreground": "#893DF6"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "meta.decorator punctuation.decorator",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "meta.decorator variable",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#7B73EA"
      }
    },
    {
      "scope": "keyword.control.at-rule",
      "settings": {
        "foreground": "#893DF6"
      }
    },
    {
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#FF4343"
      }
    },
    {
      "scope": "variable.interpolation",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "meta.source.handlebars entity.name.tag",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "punctuation.definition.expression, punctuation.definition.subexpression, punctuation.definition.block.unescaped, punctuation.definition.tag",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "entity.name.function.expression",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "entity.unescaped.expression",
      "settings": {
        "foreground": "#893DF6"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "entity.expression variable.parameter.name",
      "settings": {
        "foreground": "#DEC368"
      }
    },
    {
      "scope": "entity.expression variable.parameter.value",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "entity.expression support.function.builtin",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "entity.other.attribute-name.handlebars",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "support.class.component",
      "settings": {
        "foreground": "#FF0166",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.tag.js entity.name.tag",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#DEC368"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#DEC368",
        "font-weight": "bold"
      }
    },
    {
      "scope": "punctuation.definition.bold.markdown",
      "settings": {
        "foreground": "#FFF5D2",
        "font-weight": "bold"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#FF438E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "foreground": "#FF0166",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "punctuation.definition.heading",
      "settings": {
        "foreground": "#03DAC6"
      }
    },
    {
      "scope": "entity.name.section.markdown",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#DEC368"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#03DAC6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#FF0166"
      }
    },
    {
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#757575"
      }
    },
    {
      "scope": "fenced_code.block.language",
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.other.link",
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#7B73EA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "text.html.markdown punctuation.definition.string",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "entity.name.type.class.js",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "keyword.control.as.js",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "keyword.control.from.js",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "keyword.control.export.js",
      "settings": {
        "foreground": "#893DF6"
      }
    },
    {
      "scope": "variable.language.this.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.class.console.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.tag.js, support.class.component.js",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "variable.language.super.js",
      "settings": {
        "foreground": "#7B73EA"
      }
    },
    {
      "scope": "meta.tag.structure.any.html, meta.tag.other.html, entity.name.tag.html, meta.tag.metadata.script.html, entity.name.tag.jade",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "entity.name.tag.inline.any.html, entity.name.tag.other.html, entity.name.tag.block.any.html, entity.name.tag.localname.xml",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "entity.name.tag.xml, meta.tag.xml",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "entity.name.tag.reference.scss",
      "settings": {
        "foreground": "#E60057"
      }
    },
    {
      "scope": "keyword.other.unit.rem.css, keyword.other.unit.vh.css, keyword.other.unit.px.css, keyword.other.unit.em.css, keyword.other.unit.deg.css, keyword.other.unit.percentage.css",
      "settings": {
        "foreground": "#DEC368"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#ACDDD8"
      }
    },
    {
      "scope": "string.regexp keyword, string.regexp keyword.control",
      "settings": {
        "foreground": "#FF0166",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "string.regexp keyword.operator",
      "settings": {
        "foreground": "#FF438E"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc punctuation.definition",
      "settings": {
        "foreground": "#FFF5D2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#CDCDCD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.block storage",
      "settings": {
        "foreground": "#FFF5D2"
      }
    },
    {
      "scope": "comment.block storage.custom, variable.other.jsdoc, variable.other.jsdoc punctuation.definition.string",
      "settings": {
        "foreground": "#FF438E"
      }
    }
  ],
